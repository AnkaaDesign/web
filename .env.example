# Web Application Environment Variables - EXAMPLE
# Copy this file to .env.development, .env.staging, or .env.production
# Vite requires all client-side env vars to be prefixed with VITE_

# API Configuration
# Development: http://localhost:3030
# Staging:     https://api.staging.ankaa.live
# Production:  https://api.ankaa.live
VITE_API_URL=http://localhost:3030
VITE_API_FILES_URL=http://localhost:3030/files

# Rate Limiting (set to true to disable in development)
DISABLE_RATE_LIMITING=false
